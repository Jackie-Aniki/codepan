{
  "private": true,
  "name": "codepan",
  "productName": "CodePan",
  "description": "Play with JS/CSS/HTML so simple it hurts",
  "details": "CodePan is where people prototype front-end apps, you are free to use it offline anytime anywhere.",
  "main": "src/index.js",
  "homepage": "https://codepan.net/",
  "version": "0.2.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/Prozi/codepan"
  },
  "bugs": {
    "url": "https://github.com/Prozi/codepan/issues"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint --ext=.vue,.js -c .eslintrc.json src ./*.js --fix",
    "dev": "poi --serve",
    "deploy": "surge -p dist -d codepan.net",
    "clean": "rm -rf dist",
    "build": "NODE_ENV=production poi --prod && cp -r static/* dist",
    "start": "chef-express dist",
    "format": "npm run lint && prettier --write src"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "axios": "^0",
    "babel-preset-poi": "^10.0.2",
    "babel-preset-vue": "^2.0.2",
    "buble": "^0.20.0",
    "buble-loader": "^0.5.1",
    "codemirror": "^5",
    "codemirror-emmet": "^1.0.0",
    "core-js": "^3.34.0",
    "debounce": "^1",
    "element-ui": "^2.15.14",
    "is-electron": "^2.2.2",
    "less": "^3",
    "loadjs": "^3",
    "marked3": "^0.5.2",
    "notie": "^4.3.1",
    "nprogress": "^0.2.0",
    "object-assign": "^4.1.1",
    "offline-plugin": "^4.8.0",
    "parse-package-name": "^0",
    "pify": "^4",
    "poi": "^12.10.3",
    "poi-preset-babel-minify": "^1.0.3",
    "poi-preset-bundle-report": "^2.0.2",
    "poi-preset-offline": "^9.0.3",
    "promise-polyfill": "^8.3.0",
    "raw-loader": "^4.0.2",
    "reqjs": "^1.0.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svelte": "^1",
    "v-tippy": "^2.0.0",
    "vue": "^2",
    "vue-feather-icons": "^5.1.0",
    "vue-ga": "^1.1.0",
    "vue-router": "^3",
    "vue-slim-modal": "^1.0.4",
    "vue-template-compiler": "^2.7.16",
    "vuex": "^3",
    "webpack": "^4",
    "webpack-node-modules": "^0.1.1"
  },
  "license": "MIT",
  "dependencies": {
    "chef-express": "^2.1.3"
  },
  "optionalDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2"
  }
}
